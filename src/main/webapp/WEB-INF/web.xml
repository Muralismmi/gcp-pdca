<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/j2ee"
	xmlns:web="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	 <servlet>
    <servlet-name>FlowController</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>FlowController</servlet-name>
    <url-pattern>/</url-pattern>
  </servlet-mapping>
     <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
      
	   <security-constraint>
	        <web-resource-collection>
	            <web-resource-name>home</web-resource-name>
	            <url-pattern>/*</url-pattern>
	        </web-resource-collection>
	        <auth-constraint>
	            <role-name>*</role-name>
	        </auth-constraint>
	        <user-data-constraint>
	            <transport-guarantee>CONFIDENTIAL</transport-guarantee>
	        </user-data-constraint>
	    </security-constraint> 
    
     <security-constraint>
        <web-resource-collection>
	        <web-resource-name>removeindex</web-resource-name>
	        <url-pattern>/removeindexqueue</url-pattern>
	    </web-resource-collection>
	</security-constraint>
	<security-constraint>
        <web-resource-collection>
	        <web-resource-name>removeindex</web-resource-name>
	        <url-pattern>/readContentFromSheetQueue</url-pattern>
	    </web-resource-collection>
	</security-constraint>
		<security-constraint>
        <web-resource-collection>
	        <web-resource-name>removeindex</web-resource-name>
	        <url-pattern>/attachments/masterconfigbulkupload</url-pattern>
	    </web-resource-collection>
	</security-constraint>
		<security-constraint>
        <web-resource-collection>
	        <web-resource-name>removeindex</web-resource-name>
	        <url-pattern>/attachments/usermasterbulkupload</url-pattern>
	    </web-resource-collection>
	</security-constraint>
		<security-constraint>
        <web-resource-collection>
	        <web-resource-name>removeindex</web-resource-name>
	        <url-pattern>/attachments/pillarmasterconfigbulkupload</url-pattern>
	    </web-resource-collection>
	</security-constraint>
	<security-constraint>
        <web-resource-collection>
	        <web-resource-name>removeindex</web-resource-name>
	        <url-pattern>/attachments/linemasterconfigbulkupload</url-pattern>
	    </web-resource-collection>
	</security-constraint>
	<security-constraint>
        <web-resource-collection>
	        <web-resource-name>removeindex</web-resource-name>
	        <url-pattern>/attachments/areamasterconfigbulkupload</url-pattern>
	    </web-resource-collection>
	</security-constraint>
	<security-constraint>
        <web-resource-collection>
	        <web-resource-name>removeindex</web-resource-name>
	        <url-pattern>/attachments/toolmasterconfigbulkupload</url-pattern>
	    </web-resource-collection>
	</security-constraint>
	<security-constraint>
        <web-resource-collection>
	        <web-resource-name>removeindex</web-resource-name>
	        <url-pattern>/attachments/losstypemasterconfigbulkupload</url-pattern>
	    </web-resource-collection>
	</security-constraint>
	<security-constraint>
        <web-resource-collection>
	        <web-resource-name>mail</web-resource-name>
	        <url-pattern>/sendnotificationtonextapprover</url-pattern>
	    </web-resource-collection>
	</security-constraint>
	<security-constraint>
        <web-resource-collection>
	        <web-resource-name>removeindex</web-resource-name>
	        <url-pattern>/attachments/benefitmasterconfigbulkupload</url-pattern>
	    </web-resource-collection>
	</security-constraint>
	<security-constraint>
        <web-resource-collection>
	        <web-resource-name>logout</web-resource-name>
	        <url-pattern>/logout</url-pattern>
	    </web-resource-collection>
	</security-constraint>
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/FlowController-servlet.xml</param-value>
    </context-param>
</web-app>